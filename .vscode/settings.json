{
	"editor.formatOnSave": true,
	"todohighlight.isCaseSensitive": false,
	"todohighlight.keywords": ["TODO", "FIX", "NOTE"],
	"todohighlight.exclude": [
		"**/node_modules/**",
		"**/bower_components/**",
		"**/dist/**",
		"**/build/**",
		"**/.vscode/**",
		"**/.github/**",
		"**/_output**/",
		"**/*.min.*",
		"**/*.map",
		"**/.next/**"
	]
}
